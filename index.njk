---
title: notebook
layout:
---
<!doctype html>
<html
  lang="en"
  dir="ltr">
  <head>
    <meta charset="utf-8" />

    <link
      rel="canonical"
      href="https://hen.cat/notebook"
    />

    <meta
      name="viewport"
      content="width=device-width, initial-scale=1"
    />

    <style>
      {{ css | safe }}
    </style>

    <title>
      notebook
    </title>
  </head>
  <body>
    <main>
      <article>
        <header>
          <h1>
            {{ title }}
          </h1>
        </header>

        <dl>
          {% for note in collections.all %}
            {% if note.data.layout == "note" and note.data.name %}
              <dt>
                <a href="/{{ note.fileSlug }}">
                  {{ note.data.name }}
                </a>
              </dt>
              <dd>
                {{ note.data.description }}
              </dd>
            {% endif %}
          {% endfor %}
        </dl>

      </article>
    </main>
  </body>
</html>

