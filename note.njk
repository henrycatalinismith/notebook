---
<!doctype html>
<html
  lang="en"
  dir="ltr">
  <head>
    <meta charset="utf-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1"
    />
    <style>
      {% include "note.css" %}
    </style>
  </head>
  <body>
    <main>
      <article>
        <header>
          <h1>
            {{ name }}
          </h1>
        </header>
        {{ content | safe }}
        <footer>
          <time>
            {{ date | iso8601 }}
          </time>
        </footer>
      </article>
    </main>
  </body>
</html>

